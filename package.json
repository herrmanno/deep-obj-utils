{
  "name": "deep-obj-utils",
  "version": "1.0.0-rc.4",
  "description": "Utilities to handle immutable objects without pain",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "ava 'lib/test/**/*.js'",
    "prepublish": "npm run build && npm run test && cp lib/src/* .",
    "postpublish": "rm -f ./*.{js,js.map,d.ts}",
    "doc": "typedoc --out docs/api --mode file --excludePrivate src"
  },
  "pre-commit": [
    "build",
    "test",
    "doc"
  ],
  "keywords": [
    "getter",
    "setter",
    "deep",
    "immutable",
    "util",
    "utility",
    "utilities"
  ],
  "homepage": "https://github.com/herrmanno/deep-obj-utils",
  "author": "https://github.com/herrmanno",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "ava": "0.25.0",
    "pre-commit": "1.2.2",
    "typedoc": "0.11.1"
  }
}
